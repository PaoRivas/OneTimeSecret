{
    "env": {
        "browser": true,
        "node": true,
        "commonjs": true,
        "es2021": true,
        "jest": true
    },
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "semi": 0,
        "quotes": "off",
        "space-before-function-paren": ["error", "never"]
    }
}
